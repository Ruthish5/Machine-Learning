import csv

def find_s_algorithm(training_data):

    hypothesis = training_data[0][:-1]
    for sample in training_data:
        if sample[-1] == 'Yes':
            for i in range(len(hypothesis)):
                if sample[i] != hypothesis[i]:
                    hypothesis[i] = '?'
    return hypothesis

def read_csv(filename):
    data = []
    with open(filename, 'r') as file:
        csv_reader = csv.reader(file)
        for row in csv_reader:
            data.append(row)
    return data

def main():
    filename = '/content/enjoySport.csv'
    training_data = read_csv(filename)

    hypothesis = find_s_algorithm(training_data)

    print("The most specific hypothesis is:", hypothesis)

if __name__ == "__main__":
    main()
